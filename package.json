{
  "name": "@connectedyard/holysheet",
  "version": "2.0.7",
  "description": "Validate and map columns from one CSV file to another",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --reporter spec --require @babel/register --require @babel/polyfill --require tests/lib/globals.js tests/*Tests.js",
    "coverage": "./node_modules/nyc/bin/nyc.js --reporter=lcov npm test",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://jkennedy1980@github.com/PhinCo/csv-transform.git"
  },
  "keywords": [
    "csv",
    "transform",
    "validate",
    "map",
    "import"
  ],
  "author": "Josh Kennedy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PhinCo/csv-transform/issues"
  },
  "homepage": "https://github.com/PhinCo/csv-transform#readme",
  "dependencies": {
    "csv": "^2.0.0",
    "debug": "^3.2.6",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "papaparse": "^4.6.1",
    "request": "^2.88.0",
    "string-similarity": "^2.0.0",
    "xlsx": "^0.11.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nyc": "^11.9.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
